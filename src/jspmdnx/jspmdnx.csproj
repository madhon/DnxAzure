<Project ToolsVersion="15.0" Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <AssemblyName>jspmdnx</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>jspmdnx</PackageId>
    <AssetTargetFallback>$(PackageTargetFallback);dotnet5.6;dnxcore50;portable-net45+win8</AssetTargetFallback>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.0.7" />
    <PackageReference Include="NWebsec.AspNetCore.Mvc" Version="2.0.0" />
  </ItemGroup>
</Project>
